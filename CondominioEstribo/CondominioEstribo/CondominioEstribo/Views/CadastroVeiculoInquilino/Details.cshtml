@model CondominioEstribo.Models.CadastroVeiculoInquilino

@{
    ViewBag.Title = "Detalhes Do Veículo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<div class="container">
    <div class="well">
        <h2>Detalhes Do Veículo</h2>

        <br />
        <br />

        <div>

            <dl class="dl-horizontal">
                <dt>
                    <h4><b>Inquilino:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.NomeDoInquilino)</h4>
                </dd>

                <dt>
                    <h4><b>Placa Do Veículo:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.PlacaVeiculoInquilino)</h4>
                </dd>



            </dl>
        </div>
        <br />

        <div class="form-group">
            @Html.ActionLink("Cancelar", "Index", new { area = "Index", controller = "Home" }, new { @class = "btn btn-danger" })
            @Html.ActionLink("Alterar", "Edit", new { id = Model.CadastroVeiculoInquilinosId }, new { @class = "btn btn-warning" })                               
        </div>

    </div>
</div>
<br />
<br />