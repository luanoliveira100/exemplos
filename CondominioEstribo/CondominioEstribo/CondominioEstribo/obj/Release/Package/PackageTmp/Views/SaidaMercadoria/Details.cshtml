@model CondominioEstribo.Models.SaidaMercadoria

@{
    ViewBag.Title = "Detalhes Da Saída De Mercadoria";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes Da Saída De Mercadoria</h2>

<br />
<br />

<div>

    <dl class="dl-horizontal">
        <dt>
            <h5><b>Produto:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.ProdutoSaidaMercadoria)</h5>
        </dd>

        <dt>
            <h5><b>Quantidade:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.QuantidadeRetirada)</h5>
        </dd>

        <dt>
            <h5><b>Data Da Retirada:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.DataRetirada)</h5>
        </dd>

        <dt>
            <h5><b>Funcionário:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.FuncionarioSaidaMercadoria)</h5>
        </dd>

        <dt>
            <h5><b>Observação:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.Observacao)</h5>
        </dd>

    </dl>
</div>
<br />

<div class="row">
    <div class="col-md-1">
        @Html.ActionLink("Editar", "Edit", new { id = Model.SaidaMercadoriaId }, new { @class = "btn btn-warning" })

    </div>
    <div class="col-md-1">
        @Html.ActionLink("Cancelar", "Index", new { area = "Index", controller = "Home" }, new { @class = "btn btn-default" })
    </div>
</div>


<br />
