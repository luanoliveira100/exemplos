@model CondominioEstribo.Models.Fornecedor

@{
    ViewBag.Title = "Detalhes Do Fornecedor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<div class="container">
    <div class="well">
        <h2>Detalhes Do Fornecedor</h2>
        <br />
        <br />
        <div>
            <dl class="dl-horizontal">
                <dt>
                    <h4><b>Empresa:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.NomeEmpresa)</h4>
                </dd>

                <dt>
                    <h4><b>CNPJ/CPF:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.CNPJ_CPF)</h4>
                </dd>

                <dt>
                    <h4><b>Telefone Fixo:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.TelefoneFixo)</h4>
                </dd>

                <dt>
                    <h4><b>Telefone Celular:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.TelefoneCelular)</h4>
                </dd>

                <dt>
                    <h4><b>Endereço:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.Endereco)</h4>
                </dd>

                <dt>
                    <h4><b>Número:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.Numero)</h4>
                </dd>

                <dt>
                    <h4><b>CEP:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.CEP)</h4>
                </dd>

                <dt>
                    <h4><b>Bairro:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.Bairro)</h4>
                </dd>

                <dt>
                    <h4><b>Cidade:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.Cidade)</h4>
                </dd>

                <dt>
                    <h4><b>Estado:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.Estado)</h4>
                </dd>

                <dt>
                    <h4><b>Email Do Contato:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.EmailDoContato)</h4>
                </dd>

                <dt>
                    <h4><b>Nome Do Contato:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.NomeDoContato)</h4>
                </dd>
                <dt>
                    <h4><b>Observação:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.Observacao)</h4>
                </dd>
                <dt>
                    <h4><b>Status:</b></h4>
                </dt>

                <dd>
                    <h4>@Html.DisplayFor(model => model.FlagStatus)</h4>
                </dd>
            </dl>
        </div>

        <br />


        <div class="form-group">
            @Html.ActionLink("Cancelar", "Index", new { area = "Index", controller = "Home" }, new { @class = "btn btn-danger" })
            @Html.ActionLink("Alterar", "Edit", new { id = Model.FornecedorId }, new { @class = "btn btn-warning" })

        </div>
    </div>
</div>
<br />




   
  

