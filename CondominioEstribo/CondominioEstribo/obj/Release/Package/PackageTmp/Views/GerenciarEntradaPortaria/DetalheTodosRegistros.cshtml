@model CondominioEstribo.Models.GerenciarEntradaPortaria

@{
    ViewBag.Title = "Detalhes De Registro De Pessoas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes Do Registro De Pessoas</h2>

<br />
<br />

<div>

    <dl class="dl-horizontal">
        <dt>
            <h5><b>Nome:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.Nome)</h5>
        </dd>

        <dt>
            <h5><b>RG:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.RG)</h5>
        </dd>

        <dt>
            <h5><b>Data Da Entrada:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.DataHoraEntrada)</h5>
        </dd>

        <dt>
            <h5><b>Data Da Saída:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.DataHoraSaida)</h5>
        </dd>

        <dt>
            <h5><b>Placa Do Veículo:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.PlacaDoVeiculo)</h5>
        </dd>

        <dt>
            <h5><b>Telefone Do Contato:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.TelefoneContato)</h5>
        </dd>

        <dt>
            <h5><b>Status:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.FlagParaControle)</h5>
        </dd>

        <dt>
            <h5><b>Observação:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.Observacao)</h5>
        </dd>
    </dl>
</div>
