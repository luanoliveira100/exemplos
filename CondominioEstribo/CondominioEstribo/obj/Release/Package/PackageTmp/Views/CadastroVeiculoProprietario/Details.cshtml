@model CondominioEstribo.Models.CadastroVeiculoProprietario

@{
    ViewBag.Title = "Detalhes Do Veículo De Proprietários";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes Do Veículo De Proprietários</h2>

<br />
<br />

<div>

    <dl class="dl-horizontal">
        <dt>
            <h5><b>Nome Do Proprietário:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.NomeDoProprietario)</h5>
        </dd>

        <dt>
            <h5><b>Placa Do Veículo:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.PlacaVeiculoProprietario)</h5>
        </dd>

        

    </dl>
</div>
<br />

<div class="row">
    <div class="col-md-1">
        @Html.ActionLink("Editar", "Edit", new { id = Model.CadastroVeiculoProprietarioId }, new { @class = "btn btn-warning" })

    </div>
    <div class="col-md-1">
        @Html.ActionLink("Cancelar", "Index", new { area = "Index", controller = "Home" }, new { @class = "btn btn-default" })
    </div>
</div>


<br />
