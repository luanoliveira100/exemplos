@model CondominioEstribo.Models.CadastroVeiculoFuncionario

@{
    ViewBag.Title = "Detalhes Do Veículo De Funcionário";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes Do Veículo De Funcionário</h2>

<br />
<br />

<div>

    <dl class="dl-horizontal">
        <dt>
            <h5><b>Nome Do Funcionário:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.NomeDoFuncionario)</h5>
        </dd>

        <dt>
            <h5><b>Placa Do Veículo:</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.PlacaVeiculoFuncionario)</h5>
        </dd>

        

    </dl>
</div>
<br />

<div class="row">
    <div class="col-md-1">
        @Html.ActionLink("Editar", "Edit", new { id = Model.CadastroVeiculoFuncionarioId }, new { @class = "btn btn-warning" })

    </div>
    <div class="col-md-1">
        @Html.ActionLink("Cancelar", "Index", new { area = "Index", controller = "Home" }, new { @class = "btn btn-default" })
    </div>
</div>


<br />