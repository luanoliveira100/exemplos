@model CondominioEstribo.Models.Visitante

@{
    ViewBag.Title = "Detalhes Do Visitante";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes Do Visitante</h2>


<br />
<br />


    <div>
        <dl class="dl-horizontal">
            <dt>
                <h5><b>Nome Visitante:</b></h5>
            </dt>

            <dd>
                <h5>@Html.DisplayFor(model => model.NomeVisitante)</h5>
            </dd>

            <dt>
                <h5><b>Previsão De Entrada:</b></h5>
            </dt>

            <dd>
                <h5>@Html.DisplayFor(model => model.DataEntrada)</h5>
            </dd>

            <dt>
                <h5><b>Previsão De Saída:</b></h5>
            </dt>

            <dd>
                <h5>@Html.DisplayFor(model => model.DataSaida)</h5>
            </dd>

            <dt>
                <h5><b>Placa Do Veículo:</b></h5>
            </dt>

            <dd>
                <h5>@Html.DisplayFor(model => model.PlacaVeiculoVisitante)</h5>
            </dd>

            <dt>
                <h5><b>Nome Do Condômino:</b></h5>
            </dt>

            <dd>
                <h5>@Html.DisplayFor(model => model.NomeDoCondomino)</h5>
            </dd>

            <dt>
                <h5><b>Observação:</b></h5>
            </dt>

            <dd>
                <h5>@Html.DisplayFor(model => model.Observacao)</h5>
            </dd>

        </dl>
    </div>


<br />

<div class="row">
    @*<div class="col-md-1">
        @Html.ActionLink("Editar", "Edit", new { id = Model.VisitanteId }, new { @class = "btn btn-warning" })
    </div>*@
    <div class="col-md-1">
        @Html.ActionLink("Cancelar", "Index", new { area = "Index", controller = "Home" }, new { @class = "btn btn-default" })
    </div>
</div>


